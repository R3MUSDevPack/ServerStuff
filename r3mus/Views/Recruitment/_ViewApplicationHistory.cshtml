@model r3mus.ViewModels.ApplicationReviewViewModel

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationInfo.FirstOrDefault().DateTimeCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationInfo.FirstOrDefault().Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationInfo.FirstOrDefault().Notes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationInfo.FirstOrDefault().Screener)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.ApplicationInfo)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateTimeCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Screener)
        </td>
    </tr>
}

</table>
